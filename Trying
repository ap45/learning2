import java.sql.Timestamp;

public class CommonFieldSetter {

    public static void setCommonFields(HistoryCommonField commonFieldEntity) {
        // Set the common fields
        commonFieldEntity.setSrcout(Timestamp.valueOf("9999-01-01 00:00:00"));
        commonFieldEntity.setStgout(Timestamp.valueOf("9999-01-01 00:00:00"));
        commonFieldEntity.setStgSoftdel(/* Set your value here */);
        commonFieldEntity.setStgDel(/* Set your value here */);
        commonFieldEntity.setSrcDel(/* Set your value here */);
        commonFieldEntity.setStgCr(new Timestamp(System.currentTimeMillis()));
        commonFieldEntity.setSrcer(new Timestamp(System.currentTimeMillis()));
    }
}
